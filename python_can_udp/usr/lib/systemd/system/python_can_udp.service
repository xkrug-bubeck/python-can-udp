[Unit]
Description=XKrug System monitor service
ConditionPathExists=/opt/python_can_udp/caneth_linux.py
Requires=network-online.target systemd-modules-load.service

[Service]
ExecStart=/usr/bin/python3 /opt/python_can_udp/caneth_linux.py
StandardOutput=null
Restart=on-failure

[Install]
WantedBy=multi-user.target
